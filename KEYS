CREATE DATABASE SHOPPING;

USE SHOPPING;

CREATE TABLE PERSONS(
ID INT,
F_NAME VARCHAR(255),
ADDRESS VARCHAR(255)
);

CREATE TABLE ORDERS(
ORDER_ID INT,
ORDER_PRICE INT,
PRIMARY KEY (ORDER_ID)
);

ALTER table PERSONS MODIFY column ID int primary key;
desc persons;
desc orders;
alter table ORDERS ADD  FOREIGN KEY(ORDER_ID) REFERENCES PERSONS(ID);
alter table ORDERS ADD  FOREIGN KEY(ORDER_PRICE) REFERENCES PERSONS(ID);
